{
    "manifest_version": 3,
    "name": "Gmail Chrome OS Notifications",
    "version": "1.0",
    "description": "A tool to add an app badge on Chrome OS",
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "icons": {
        "128": "images/icon128.png"
    }
}